{
  "rewrites": [
    {
      "source": "/api/backend/:path*",
      "destination": "https://insidr-production.up.railway.app/api/:path*"
    },
    {
      "source": "/auth/:path*",
      "destination": "https://insidr-production.up.railway.app/api/v1/auth/:path*"
    },
    {
      "source": "/api/webhook",
      "destination": "https://insidr-production.up.railway.app/webhook"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
